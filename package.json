{
  "name": "nextjs",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "local": "next",
    "dev": "yarn && next build && next start",
    "prod": "yarn && NEXT_PUBLIC_NODE_ENV=production next build &&  next start -p 5000",
    "prodLocal": "NEXT_PUBLIC_NODE_ENV=production next -p 5000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint --ext .jsx --ext .js --fix --ignore-path .eslintignore .",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn eslint",
      "git add --force"
    ]
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@feathersjs/authentication-client": "^4.5.13",
    "@feathersjs/feathers": "^4.5.12",
    "@feathersjs/rest-client": "^4.5.12",
    "@feathersjs/socketio-client": "^4.5.13",
    "@mui/icons-material": "5.8.0",
    "@mui/lab": "^5.0.0-alpha.77",
    "@mui/material": "latest",
    "axios": "^0.25.0",
    "cookie-storage": "^6.1.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "material-ui-popup-state": "^2.0.0",
    "next": "latest",
    "next-intl": "^2.4.1",
    "next-optimized-images": "^2.6.2",
    "notistack": "^2.0.3",
    "prop-types": "latest",
    "react": "latest",
    "react-cropper": "^1.3.0",
    "react-dom": "latest",
    "react-infinite-scroller": "^1.2.6",
    "react-quill": "^1.3.5",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "@next/eslint-plugin-next": "^11.1.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  }
}
